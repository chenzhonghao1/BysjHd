<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.biyesheji.find.dao.LoginMapper">
    <!--登录-->
    <select id="login" parameterType="com.biyesheji.find.qo.UserQo" resultType="java.lang.Boolean">
        <![CDATA[ select count(*)
        from user
        where username = #{username}
        AND password = #{password}
        ]]>
    </select>

</mapper>